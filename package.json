{
  "name": "base-express-sequelize",
  "version": "1.0.0",
  "description": "This is a base for express es6 and building with sequelize ORM",
  "main": "main.js",
  "keywords": [
    "nodejs",
    "express",
    "rest",
    "api",
    "sequelize",
    "mysql"
  ],
  "author": "Đặng Phú",
  "license": "MIT",
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "scripts": {
    "lint": "eslint --fix --ext .js .",
    "dev": "nodemon --exec babel-node ./src/bin/www.js",
    "start": "node ./dist/bin/www.js",
    "clean": "rm -rf dist && mkdir dist",
    "build:babel": "babel ./src --out-dir dist --copy-files",
    "build": "yarn run clean && yarn run build:babel",
    "model": "sequelize-cli model:generate --name",
    "seed": "sequelize-cli seed:generate --name",
    "migrate:generate": "sequelize-cli migration:generate --name",
    "migrate:revert": "sequelize-cli db:migrate:undo --name",
    "migrate:latest": "sequelize-cli db:migrate",
    "migrate:rollback": "sequelize-cli db:migrate:undo:all",
    "migrate:reset": "yarn migrate:rollback && yarn migrate:latest && yarn seed:run",
    "seed:one": "sequelize db:seed",
    "seed:run": "sequelize db:seed:all",
    "seed:down": "sequelize db:seed:undo",
    "crawl:start": "node ./src/database/seeders/factory/crawl/index.js",
    "heroku-postbuild": "yarn install && yarn migrate:reset && yarn run build"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cheerio": "^1.0.0-rc.3",
    "cloudinary": "^1.22.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.0",
    "fs": "^0.0.1-security",
    "http-status": "^1.4.2",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.10",
    "passport": "^0.4.1",
    "path": "^0.12.7",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "puppeteer": "^5.2.1",
    "sequelize": "^6.2.0",
    "speakingurl": "^14.0.1",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@babel/runtime": "^7.11.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "nodemon": "^2.0.4",
    "sequelize-cli": "^6.0.0"
  }
}
